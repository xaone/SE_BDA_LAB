<%- include('partials/iheader.ejs') %>
<style>
    html,body{
    
      width: 100%;
      height:100%; 
    }
    table {
        width: 100%;
        text-align: center;
    }
    tr{
        color:black;
        font-family:'Poppins';
        border-bottom:1px solid black;
    }
    th{
        color:#2fb5ae;
        font-family:'Poppins';

    }
    a:link {
        color: #2fb5ae;
        background-color: transparent;
        text-decoration: none;
    }
    h2{
       color:black;
       text-align:center; 
    }
    
    /* body{
     margin: 0; 
    
} */
    
</style>
  <!-- Sidenav -->
<!-- End -->

<!-- Main -->
<div class="main">
    <h2>Profile</h2>
    <div class="card">
        <div class="card-body">
            <table>
                <tbody>
                    <tr>
                        <td>Name</td>
                        <td>:</td>
                        <td><%= m.name %></td>
                    </tr>
                    <tr>
                        <td>Position</td>
                        <td>:</td>
                        <td><%= m.position %></td>
                    </tr>
                    <tr>
                        <td>Enrollment No</td>
                        <td>:</td>
                        <td><%= m.enroll %></td>
                    </tr>
                    <tr>
                        <td>Subjects</td>
                        <td>:</td>
                        <td><%= m.subjects %></td>
                    </tr>
                
                    
                </tbody>
            </table>
            <h2 style="text-align:center;">Publications in BDA LAB</h2>
    <table  id="table2">
        <thead>
          <tr>
                <th>Authors</th>
                <th>Publication in MLA format</th>
                <th>Title</th>
                <th>PDF URL/ LINK</th>        
          </tr>
        </thead>
        <tbody>
          <%list.forEach((article)=>{%>
            <tr>
                <td><%= article.author1 %>,<%= article.author2 %>,<%= article.author3 %></td>
                <td><%= article.mla %></td>
                <td><%= article.title %></td>
                <td><a href="<%= article.pdf %>">PDF</a></td> 
            </tr>   
            <%})%>
        </tbody>
        </table>
        </div>
        
    </div>
<%- include('partials/ifooter.ejs') %>